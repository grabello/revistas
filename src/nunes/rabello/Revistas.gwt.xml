<module>

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
	
      <!-- Inherit the default GWT style sheet.  You can change       -->
      <!-- the theme of your GWT application by uncommenting          -->
      <!-- any one of the following lines.                            -->
      <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
       <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> 
      <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->    

      <!-- Other module inherits                                      -->


      <!-- Specify the app entry point class.                         -->
      <entry-point class='nunes.rabello.client.Revistas'/>
      
      <servlet path="/addTitleServlet" class="nunes.rabello.server.servlets.AddTitleServlet" />
      <servlet path="/addNumberServlet" class="nunes.rabello.server.servlets.AddNumberServlet" />
      <servlet path="/addStuffServlet" class="nunes.rabello.server.servlets.AddStuffServlet" />
      <servlet path="/gettitle"	class="nunes.rabello.server.implementations.TitleServiceImpl" />
      <servlet path="/getlist"	class="nunes.rabello.server.implementations.ListServiceImpl" />
    
      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Revistas.css' />
	
</module>
